{
  "name": "mathalea",
  "version": "2.1.0",
  "description": "Générateur d'exercices de mathématiques",
  "main": "mathalea.js",
  "scripts": {
    "lint": "eslint",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "start": "webpack serve",
    "watch": "webpack --watch"
  },
  "dependencies": {
    "algebrite": "^1.4.0",
    "core-js": "^3.10.0",
    "instrumenpoche": "git+https://framagit.org/Sesamath/instrumenpoche.git",
    "jquery": "^3.6.0",
    "jquery-ui": "^1.12.1",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.13.15",
    "babel-loader": "^8.2.2",
    "browserslist": "^4.16.3",
    "css-loader": "^5.2.1",
    "eslint": "^7.23.0",
    "eslint-config-standard": "16.0.2",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "mini-css-extract-plugin": "^1.4.1",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.33.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remiangot/MathALEA.git"
  },
  "keywords": [
    "math",
    "exercise",
    "javascript"
  ],
  "author": "à remplir, cf https://docs.npmjs.com/cli/v6/configuring-npm/package-json#people-fields-author-contributors",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/remiangot/MathALEA/issues"
  },
  "homepage": "https://github.com/remiangot/MathALEA#readme",
  "babel": {
    "plugins": [
      "syntax-dynamic-import"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": {
            "version": "3.8",
            "proposals": true
          }
        }
      ]
    ],
    "sourceType": "unambiguous"
  },
  "browserslist": [
    "supports es6-module-dynamic-import"
  ],
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": [
      "standard"
    ]
  }
}
