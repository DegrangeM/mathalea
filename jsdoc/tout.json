{
  "comment"  : "cf http://usejsdoc.org/about-configuring-jsdoc.html",
  "tags"     : {
    "allowUnknownTags": true
  },
  "source"   : {
    "include": [
      "./src/js"
    ],
    "exclude": [
      "./src/js/modules/2d.js",
      "./src/js/modules/2dAnimation.js",
      "./src/js/modules/2dGeneralites.js",
      "./src/js/modules/2dInstruments.js",
      "./src/js/modules/2dinteractif.js",
      "./src/js/modules/2dLutin.js",
      "./src/js/modules/3d.js",
      "./src/js/modules/aleaFigure",
      "./src/js/modules/Alea2iep.js",
      "./src/js/modules/ElementInstrumenpoche",
      "./src/js/modules/iepMacros"
    ]
  },
  "plugins"  : ["plugins/markdown"],
  "markdown" : {
    "idInHeadings": true
  },
  "opts"     : {
    "destination": "./documentation/tout",
    "recurse"    : true,
    "private"    : false,
    "template": "node_modules/docdash"
  },
  "docdash": {
    "comment": "Cf https://github.com/clenemt/docdash (options)",
    "static": true,
    "sort": true,
    "search": true,
    "collapse": true,
    "commonNav": true
  },
  "templates": {
    "cleverLinks": true
  }
}
